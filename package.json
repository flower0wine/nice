{
  "name": "nice",
  "displayName": "Nice",
  "version": "1.1.0",
  "description": "现在开上快车道，让你对网页的操作再无限制😁🎈🎉",
  "author": {
    "name": "flower0wine",
    "email": "flowerwine8023@gmail.com",
    "url": "https://github.com/flower0wine"
  },
  "publishConfig": {
    "@flower0wine:registry": "https://npm.pkg.github.com"
  },
  "license": "GPL-3.0",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,scss,md,js}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/generator": "^7.26.3",
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "@babel/types": "^7.26.3",
    "@plasmohq/messaging": "^0.6.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tabler/icons-react": "^3.22.0",
    "core-js": "^3.39.0",
    "framer-motion": "^11.11.17",
    "localforage": "^1.10.0",
    "next-themes": "^0.4.3",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.0",
    "prettier": "3.2.4",
    "sass": "^1.81.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*",
      "chrome-devtools://*/*",
      "view-source:*"
    ],
    "permissions": [
      "scripting",
      "activeTab",
      "storage",
      "tabs",
      "webNavigation",
      "webRequest",
      "declarativeNetRequest",
      "declarativeNetRequestWithHostAccess"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "../rules.json"
        }
      ]
    },
    "web_accessible_resources": [
      {
        "resources": [
          "log.*.js",
          "remove_fn.*.js",
          "remove_keyboard.*.js",
          "reset_keyboard.*.js",
          "remove_alert.*.js",
          "reset_alert.*.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../contents/log.ts"
        ],
        "run_at": "document_start",
        "all_frames": true
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../contents/embedded.ts"
        ],
        "run_at": "document_start",
        "all_frames": true
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../contents/alert.ts"
        ],
        "run_at": "document_start"
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../contents/keyboard.ts"
        ],
        "run_at": "document_start",
        "all_frames": true
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../contents/debuger.ts"
        ],
        "run_at": "document_start",
        "all_frames": true
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../contents/fn.ts"
        ],
        "run_at": "document_start",
        "all_frames": true
      }
    ],
    "background": {
      "service_worker": "../background/index.ts"
    }
  }
}
